@using Homework_1st.Models;
@using Homework_1st.ViewModels;

@model List<User>

<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">First</th>
            <th scope="col">Last</th>
            <th scope="col">Handle</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            @foreach (var user in Model)
            {
                <th scope="row">1</th>
                <td>@user.FullName</td>
                <td>@user.Email.</td>
                <td>@user.Age</td>

            }

        </tr>
    </tbody>
</table>
