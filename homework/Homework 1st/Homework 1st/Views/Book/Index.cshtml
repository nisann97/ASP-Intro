@model List<BookVM>


<div class="container mt-5">

    <table class="table">
        <thead>
            <tr>
                <th scope="col">Name</th>
               
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Name</td>
                 
                </tr>
            }

        </tbody>
    </table>
</div>

